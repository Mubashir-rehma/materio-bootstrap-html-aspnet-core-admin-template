@{
  TempData["creatorName"] = "ThemeSelection";
  TempData["creatorUrl"] = "https://themeselection.com";
  TempData["templateName"] = "MyAdmin";
  TempData["templateSuffix"] = "Bootstrap Admin Template";
  TempData["templateVersion"] = "1.0.0";
  TempData["templateFree"] = false;
  TempData["templateDescription"] = "Start your development with a Dashboard for Bootstrap 5";
  TempData["templateKeyword"] = "dashboard, bootstrap 5 dashboard, bootstrap 5 design, bootstrap 5";
  TempData["licenseUrl"] = "https://themeselection.com/license/";
  TempData["livePreview"] = "https://themeselection.com/demo/chameleon-admin-template/landing/";
  TempData["productPage"] = "https://themeselection.com/products/chameleon-admin-modern-bootstrap-webapp-dashboard-html-template-ui-kit/";
  TempData["support"] = "https://themeselection.com/support/";
  TempData["moreThemes"] = "https://themeselection.com/";
  TempData["documentation"] = "https://themeselection.com/documentation";
  TempData["generator"] = "https://themeselection.com/tools/generator/sneat";
  TempData["changelog"] = "https://themeselection.com/changelog.html";
  TempData["repository"] = "https://gitlab.com/repository";
  TempData["gitRepo"] = "chameleon-free-bootstrap-admin-template";
  TempData["gitRepoAccess"] = "https://themeselection.com/tools/github/github-access";
  TempData["githubFreeUrl"] = "https://github.com/themeselection";
  TempData["facebookUrl"] = "https://www.facebook.com/ThemeSelections/";
  TempData["twitterUrl"] = "https://twitter.com/Theme_Selection";
  TempData["githubUrl"] = "https://github.com/themeselection";
  TempData["dribbbleUrl"] = "https://dribbble.com/themeselection";
  TempData["instagramUrl"] = "https://www.instagram.com/themeselection/";
}

<!DOCTYPE html>
<html lang="en" class='light-style @ViewData["navbarFixed"] @ViewData["menuFixed"] @ViewData["menuCollapsed"] @ViewData["menuFlipped"] @ViewData["menuOffcanvas"] @ViewData["footerFixed"] @ViewData["customizerHidden"]' dir="ltr" data-theme="theme-default" data-assets-path="/" data-template='@((@ViewData["menuHorizontal"] != null && Convert.ToBoolean(@ViewData["menuHorizontal"])) ? "horizontal-menu-template" : "vertical-menu-template")'>

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

  <title>@ViewData["title"] | @TempData.Peek("templateName") - @TempData["templateSuffix"]</title>
  <meta name="description" content='@TempData["templateDescription"]' />
  <meta name="keywords" content='@TempData["templateKeyword"]' />
  <!-- Canonical SEO -->
  <link rel="canonical" href='@TempData["productPage"]'>
  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="~/img/favicon/favicon.ico" />

  <!-- Core Styles -->
  @await Html.PartialAsync("Layouts/Sections/_Styles")

  <!-- Vendor Styles -->
  @RenderSection("VendorStyles", required: false)

  <!-- Page Styles -->
  @RenderSection("PageStyles", required: false)

  <!-- Include Scripts for customizer, helper, analytics, config -->
  @await Html.PartialAsync("Layouts/Sections/_ScriptsIncludes")
</head>

<body>
  <!-- Layout Content -->
  @RenderBody()
  <!--/ Layout Content -->

  <div class="buy-now">
    <a href='@TempData["productPage"]' target="_blank" class="btn btn-danger btn-buy-now">Buy Now</a>
  </div>

  <!-- Core Scripts -->
  @await Html.PartialAsync("Layouts/Sections/_Scripts")

  <!-- Vendor Scripts -->
  @RenderSection("VendorScripts", required: false)

  <script src="~/js/main.dist.js"></script>

  <!-- Page Scripts-->
  @RenderSection("PageScripts", required: false)

  <script>
    feather.replace();
  </script>


</body>

</html>