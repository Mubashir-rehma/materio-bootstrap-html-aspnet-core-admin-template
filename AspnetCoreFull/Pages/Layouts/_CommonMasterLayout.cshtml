@{

  TempData["appName"] = "MyAdmin";
  TempData["appUrl"] = "https://themeselection.com";
  @* Footer & Misc urls *@
  TempData["licenseUrl"] = "https://themeselection.com/license/";
  TempData["productPage"] = "https://themeselection.com/products/chameleon-admin-modern-bootstrap-webapp-dashboard-html-template-ui-kit/";
  TempData["support"] = "https://themeselection.com/support/";
  TempData["moreThemes"] = "https://themeselection.com/";
  TempData["documentation"] = "https://themeselection.com/documentation";
}

<!DOCTYPE html>
<html lang="en" class='light-style @ViewData["navbarFixed"] @ViewData["menuFixed"] @ViewData["menuCollapsed"] @ViewData["menuFlipped"] @ViewData["menuOffcanvas"] @ViewData["footerFixed"] @ViewData["customizerHidden"]' dir="ltr" data-theme="theme-default" data-assets-path="/" data-template='@((Convert.ToBoolean(@TempData.Peek("menuHorizontal")).ToString() == "True") ? "horizontal-menu-template" : "vertical-menu-template")'>

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

  <title>@ViewData["title"] | @TempData.Peek("appName") - Asp.Net Core Admin Template</title>
  <meta name="description" content="" />

  <!-- Canonical SEO -->
  <link rel="canonical" href='@TempData.Peek("productPage")'>
  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="~/img/favicon/favicon.ico" />

  <!-- Core Styles -->
  @await Html.PartialAsync("Layouts/Sections/_Styles")

  <!-- Vendor Styles -->
  @RenderSection("VendorStyles", required: false)

  <!-- Page Styles -->
  @RenderSection("PageStyles", required: false)

  <!-- Include Scripts for customizer, helper, analytics, config -->
  @await Html.PartialAsync("Layouts/Sections/_ScriptsIncludes")
</head>

<body>
  <!-- Layout Content -->
  @RenderBody()
  <!--/ Layout Content -->

  <div class="buy-now">
    <a href='@TempData.Peek("productPage")' target="_blank" class="btn btn-danger btn-buy-now">Buy Now</a>
  </div>

  <!-- Core Scripts -->
  @await Html.PartialAsync("Layouts/Sections/_Scripts")

  <!-- Vendor Scripts -->
  @RenderSection("VendorScripts", required: false)

  <script src="~/js/main.dist.js"></script>

  <!-- Page Scripts-->
  @RenderSection("PageScripts", required: false)

  <script>
    feather.replace();
  </script>


</body>

</html>